{
  "name": "gantt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npx rimraf ./dist/wintermoon && npx ng build @wintermoon/gantt",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "cd projects/wintermoon/gantt && npx rimraf ../../../docs && npx compodoc -p tsconfig.lib.json -d ../../../docs -c .compodocrc.json && cd -",
